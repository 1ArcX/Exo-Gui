game.Players.LocalPlayer.character.RightHand.Aura1: Destroy()
game.Players.LocalPlayer.character.LeftHand.Aura1: Destroy()
game.Players.LocalPlayer.character.LeftUpperLeg.Aura1: Destroy()
game.Players.LocalPlayer.character.RightUpperLeg.Aura1: Destroy()
game.Players.LocalPlayer.character.UpperTorso.Aura1: Destroy()
game.Players.LocalPlayer.character.UpperTorso.Aura2: Destroy()
end)

local Tab = Window:NewTab("Misc")

local SectionMisc = Tab:NewSection("-ForceFieldShield-")

SectionMisc:NewButton("Activate", "Equip PPtool and talk to GhostRider then activate", function()
stopQuest = true;

function NoKill()
    local Character = game:GetService'Players'.LocalPlayer.Character
    local mt = getrawmetatable(game)
    local OldIndex = mt.__index
    local OldNameCall = mt.__namecall
    
    setreadonly(mt, false)
    
    mt.__namecall = newcclosure(function(Self,...)
        local args = {...}
        local NameCallMethod = getnamecallmethod()
        if not checkcaller() and Self == Character and NameCallMethod == "BreakJoints" then
            return nil
        end
        return OldNameCall(Self,...)  
    
    end)
    
    setreadonly(mt, true)
end

function fixCam()
    local CurrentCamera = workspace.CurrentCamera;
    CurrentCamera.CameraSubject = game.Players['LocalPlayer'].Character.Humanoid
    CurrentCamera.CameraType = Enum.CameraType.Custom
end

function SetSpeed(speed)
    local isSpeed = assert(tonumber(speed), 'Must be a number')
   game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(speed) 
end

function CanAttack()
    local MainScript = game:GetService("Players").LocalPlayer.PlayerScripts.MainScript
    
    while stopQuest do
        wait(0.01)
        getsenv(MainScript).TalkingOnQuest = false;
    end
end

NoKill()
fixCam()
CanAttack()
end)

local SectionMisc2 = Tab:NewSection("-AntiAfk-")

SectionMisc2:NewButton("Activate", "You will not get kicked after 20 minutes", function()
loadstring(game:HttpGet("https://pastebin.com/raw/DismK6uy", true))()
end)

local SectionMisc4 = Tab:NewSection("-Remove forcefield after respawn-")


local ff 
SectionMisc4:NewToggle("Remove forcefield", "Removing ff shield after respawn", function(state)
    ff = state
    while ff do
    wait()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "ForceField" then
          v:Destroy()
        end
    end
end
end)

local SectionMisc6 = Tab:NewSection("-Lag spawn menu (Fly)-")

local LSM 
SectionMisc6:NewToggle("Lag spawn menu", "Use fly and move your camera away", function(state)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(705.671021, 941.897827, 864.893188, 0.459262937, -0.712803423, 0.530083895, -0, 0.596739471, 0.8024351, -0.888300419, -0.368528694, 0.274060309)
    LSM = state
    while LSM do
wait(0.01)
local args = {
    [1] = {
        [1] = "Skill_KillingIntent_Start"
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end
end)

local SectionMisc6 = Tab:NewSection("-Spam killing intent-")

local ki 
SectionMisc6:NewToggle("Start spamming", "move your camera away", function(state)
    ki = state
    while ki do
wait(0.01)
local args = {
    [1] = {
        [1] = "Skill_KillingIntent_Start"
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end
end)
