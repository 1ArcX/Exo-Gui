local admins = {635394516;
270910992;
1310998423;
1163780619;

}


local player = game.Players.LocalPlayer

if not table.find(admins,player.UserId) then
    player:Kick("You are not whitelisted.")
end

local playerlist = {};
for i,v in pairs(game.Players:GetPlayers())do
table.insert(playerlist,v.Name)
end

function SoulAttack(Random)
    spawn(function()
        game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_Start",Random})
        game.ReplicatedStorage.RemoteEvent:FireServer({"Skill_SoulAttack_End"})
    end)
end

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()


local Window = Library.CreateLib("ExoGUI v 1.7 Beta", "Ocean")

local Tab = Window:NewTab("Credits")

local Section = Tab:NewSection("Credits to KrendelOK#3962 (Hy6aCuHa)")
local Sectioncred = Tab:NewSection("Credits to _Pablo_#6554 (PabloPVPtv)")
local Sectioncred2 = Tab:NewSection("Credits to X")
Sectioncred2:NewKeybind("Turn ON/OFF GUI", "why are you reading this", Enum.KeyCode.RightAlt, function()
	Library:ToggleUI()
end)


local Tab = Window:NewTab("Player")

local SAA = Tab:NewSection("Target")

local PlayerChosen;
local drop = SAA:NewDropdown("Player", "Other script users aren't in here", playerlist, function(new)
    PlayerChosen = new;
end)

game.Players.PlayerAdded:Connect(function(player)
    local name = player.Name
        table.insert(playerlist,name)
        print(player.Name)
    drop:Refresh(playerlist)
end)

game.Players.PlayerRemoving:Connect(function(player)
    local name = player.Name
    for i,v in pairs(playerlist)do
        if v == name then  
            table.remove(playerlist,i)
            warn(player.Name)
        end
    end
    drop:Refresh(playerlist)
end)


local sr
SAA:NewToggle("Loop Soul reap", "Soul reaps", function(state)
sr = state
while sr do
wait(0.1)
local args = {
    [1] = {
        [1] = "Skill_SoulReap",
        [2] = game.Players:FindFirstChild(PlayerChosen)
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
end
end)

SAA:NewKeybind("Void", "teleports your target to void", Enum.KeyCode.K, function()
local target = game.Players:FindFirstChild(PlayerChosen)
            local plr = game.Players.LocalPlayer
            plr.Character.Humanoid.Name = 1
            local l = plr.Character["1"]:Clone()
            l.Parent = plr.Character
            l.Name = "Humanoid"
            wait(.2)
            local args = {[1] = {[1] = "Respawn"}}
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            plr.Character["1"]:Destroy()
            workspace.CurrentCamera.CameraSubject = plr.Character
            plr.Character.Humanoid.DisplayDistanceType = "None"
            plr.Character.Humanoid:UnequipTools()
            local v = plr.Backpack:FindFirstChildOfClass("Tool")
            v.Parent = plr.Character
            v.Parent = plr.Character.HumanoidRootPart
            firetouchinterest(target.Character.HumanoidRootPart, v.Handle, 0)
            firetouchinterest(target.Character.HumanoidRootPart, v.Handle, 1)
            plr.Character.HumanoidRootPart.CFrame = CFrame.new(0, workspace.FallenPartsDestroyHeight + 5, 0)
end)

SAA:NewKeybind("Bring", "teleports your target to you", Enum.KeyCode.L, function()
local target = game.Players:FindFirstChild(PlayerChosen)
            local plr = game.Players.LocalPlayer
            plr.Character.Humanoid.Name = 1
            local l = plr.Character["1"]:Clone()
            l.Parent = plr.Character
            l.Name = "Humanoid"
             local args = {[1] = {[1] = "Respawn"}}
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            wait(.2)
            plr.Character["1"]:Destroy()
            workspace.CurrentCamera.CameraSubject = plr.Character
            plr.Character.Humanoid.DisplayDistanceType = "None"
            plr.Character.Humanoid:UnequipTools()
            local v = plr.Backpack:FindFirstChildOfClass("Tool")
            v.Parent = plr.Character
            v.Parent = plr.Character.HumanoidRootPart
            firetouchinterest(target.Character.HumanoidRootPart, v.Handle, 0)
            firetouchinterest(target.Character.HumanoidRootPart, v.Handle, 1)
            local SavePosition = Instance.new("TextLabel")
            lastlocationx = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.x
            lastlocationy = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.y
            lastlocationz = game.Players.LocalPlayer.Character.HumanoidRootPart.Position.z
            SavePosition.Text = "Last Place: " ..lastlocationx.. "," ..lastlocationy.. "," ..lastlocationz.. ","
       wait(1.5)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(lastlocationx, lastlocationy, lastlocationz)
end)

