local Tab = Window:NewTab("Loops")


local Section9 = Tab:NewSection("-All loops-")
local SAA 
Section9:NewToggle("Soul Attack All", "Soul Attacks all players", function(state)
    SAA = state
    while SAA do
    for i,v in pairs(game.Players:GetPlayers()) do
        local Random = game.Players:GetPlayers()[math.random(1,#game.Players:GetPlayers())]
        if Random.PrivateStats.PsychicPower.Value <= game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value/10 then
           SoulAttack(Random);
        end
    end
    wait(0)
    end
end)


