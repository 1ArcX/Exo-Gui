        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2040.67151, 714.274963, -1883.12024)
elseif tonumber(string.format("%.0f", game.Players.LocalPlayer.PrivateStats.BodyToughness.Value)) >= 4e06 then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2299.5437, 977.341797, 1068.81091)
